{
  "expo": {
    "name": "mprojectmanager",
    "slug": "mprojectmanager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/mprojectmanager-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mkanban.mprojectmanager"
    },
    "android": {
      "package": "com.mkanban.mprojectmanager",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/mprojectmanager-adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "MANAGE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/mprojectmanager-favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-web-browser",
      "./plugins/withStoragePermission.js"
    ]
  }
}
